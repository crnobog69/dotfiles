{
    "security.workspace.trust.untrustedFiles": "open",
    "tabnine.experimentalAutoImports": true,
    "workbench.productIconTheme": "material-product-icons",
    "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\gcc.exe",
    "window.titleBarStyle": "custom",
    "editor.linkedEditing": true,
    "vscodeGoogleTranslate.preferredLanguage": "Serbian",
    "settingsSync.ignoredExtensions": [
        "illixion.vscode-vibrancy-continued"
    ],
    "workbench.editorAssociations": {
        "*.mp3": "vscode.audioPreview",
        "*.jpg": "luna.editor"
    },
    "editor.inlineSuggest.suppressSuggestions": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": null
    },
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    },
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "catppuccin.colorOverrides": {},
    "catppuccin.accentColor": "green",
    "workbench.iconTheme": "catppuccin-mocha",
    "glassit.alpha": 255,
    "editor.fontFamily": "'JetBrainsMono Nerd Font'",
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "editor.fontSize": 15,
    "supermaven.enableFixWithSupermaven": true,
    "editor.formatOnSave": true,
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "editor.tokenColorCustomizations": {
        "[Catppuccin Mocha]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "keyword.control",
                        "keyword.operator.new",
                        "keyword.operator.expression",
                        "keyword.operator.logical.and",
                        "storage.type",
                        "storage.modifier",
                        "entity.name.function"
                    ],
                    "settings": {
                        "fontStyle": "italic"
                    }
                }
            ]
        }
    },
    "github.copilot.editor.enableAutoCompletions": false,
    "catppuccin.syncWithIconPack": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.editor.enablePreview": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "workbench.startupEditor": "none",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "workbench.colorTheme": "Catppuccin Mocha"
}
